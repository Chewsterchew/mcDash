        }        if(!(isset($settings['pp-iteminhand']) and $settings['pp-iteminhand'] == 'false'))        {            echo '            <fieldset>                <legend>Item In Hand</legend>                '.mcitemname($ver[0]->success->itemInHand->type, $ver[0]->success->itemInHand->dataValue).' <img src="dpnd/images/mcitems/'.mcitemname($ver[0]->success->itemInHand->type, $ver[0]->success->itemInHand->dataValue, true).'.png" id="blkimg"></img>            </fieldset>            ';
