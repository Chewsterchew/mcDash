        }        if(!(isset($settings['pp-currentexperience']) and $settings['pp-currentexperience'] == 'false'))        {            echo '            <fieldset>                <legend>Current Experience</legend>                '.$ver[0]->success->experience.'            </fieldset>            '; 
        }        if(!(isset($settings['pp-currentworld']) and $settings['pp-currentworld'] == 'false'))        {            echo '            <fieldset>                <legend>Current World</legend>                '.$ver[0]->success->worldInfo->name.'            </fieldset>            ';
        }        if(!(isset($settings['pp-gamemode']) and $settings['pp-gamemode'] == 'false'))        {            echo '            <fieldset>                <legend>Gamemode</legend>                '.$ver[0]->success->gameMode.'            </fieldset>            ';
