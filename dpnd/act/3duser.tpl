   ';                if($num % 9 === 0)                {                    echo '</tr>';                }            }            echo '</table>';        }        //////////////////////////////////////////        if(!(isset($settings['pp-status']) and $settings['pp-status'] == 'false'))        {            echo '            <fieldset>                <legend>Status</legend>';                if(isset($ver[0]->success->ip) and $ver[0]->success->ip !== 'offline')                {                    echo 'Online';                }                else                {                    echo 'Offline';                }                echo '            </fieldset>            ';
